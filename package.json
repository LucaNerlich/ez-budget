{
    "name": "ez-budget.de",
    "version": "2.11.0",
    "private": true,
    "repository": "https://gitlab.com/nerlich/ez-budget",
    "author": "Luca Nerlich <luca.nerlich@gmail.com>",
    "description": "EZ-Budget",
    "homepage": "https://ezbudget.de",
    "scripts": {
        "upgrade": "npx npm-check-updates -u && npm i",
        "dev": "next -p 3000",
        "build": "next build",
        "lint": "next lint",
        "start": "next start -p 3456",
        "postversion": "npm i &&  git add . && git commit -m 'bump version' && git push",
        "test": "npm run jest && npm run cypress:e2e:headless",
        "pm2": "cross-env NODE_ENV=production pm2 start npm --name 'ezbudget' -- run start",
        "jest": "jest",
        "cypress": "cypress open",
        "cypress:headless": "cypress run --headless --browser chrome --config video=false,screenshotOnRunFailure=false",
        "cypress:e2e": "start-server-and-test start http://localhost:3456 cypress",
        "cypress:e2e:headless": "start-server-and-test start http://localhost:3456 cypress:headless"
    },
    "engines": {
        "node": "18.15.0"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.8",
        "axios": "^1.6.0",
        "bootstrap": "^5.3.2",
        "chart.js": "^3.9.1",
        "chroma-js": "^2.4.2",
        "cross-env": "^7.0.3",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "next": "^14.0.0",
        "react": "^18.2.0",
        "react-chartjs-2": "^4.3.1",
        "react-cookie": "^4.1.1",
        "react-dom": "^18.2.0",
        "react-loader-spinner": "^5.4.5",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "cypress": "^10.11.0",
        "eslint": "^8.52.0",
        "eslint-config-next": "^14.0.0",
        "jest": "^29.7.0",
        "sass": "^1.69.5",
        "start-server-and-test": "^1.15.4",
        "typescript": "^4.9.5"
    }
}
